package 콘서트예약;
import java.util.Scanner;
class Concert{
	private Scanner sc;
	
	public Concert() {
		new Line().InitialName();
		sc=new Scanner(System.in);
	}
	
	void reservation() {
		System.out.print("좌석구분 S:1, A:2, B:3 >> ");
		int user=sc.nextInt();
		switch(user) {
		case 1:
			new Line().SseatRun();
			break;
		case 2:
			new Line().AseatRun();
			break;
		case 3:
			new Line().BseatRun();
			break;
		}
	}
	
	void Lookup() {
		
	}
	
	void cancel() {
		
	}
}